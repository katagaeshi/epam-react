{
  "name": "react-app",
  "version": "1.0.0",
  "description": "epam react course",
  "repository": {
    "type": "git",
    "url": "https://github.com/katagaeshi/epam-react.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "nightwatch --env default",
    "start": "node ./src/app.js",
    "webpack:bundle:dev": "node_modules/.bin/webpack --env.NODE_ENV=development",
    "webpack:bundle:prod": "node_modules/.bin/webpack --env.NODE_ENV=production",
    "webpack:server:dev": "node_modules/.bin/webpack-dev-server --env.NODE_ENV=development",
    "webpack:server:prod": "node_modules/.bin/webpack-dev-server --env.NODE_ENV=production"
  },
  "author": "Yauheni Kireyeu",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^2.38.3",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "nightwatch": "^0.9.21",
    "react-test-renderer": "^16.3.2",
    "redux": "^4.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "rootDir": "./test/unit",
    "modulePaths": [
      "<rootDir>/../../"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
